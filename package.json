{
  "name": "@voiceflow/natural-language-commander",
  "version": "0.4.3",
  "description": "A tool for connecting natural language commands with callbacks.",
  "homepage": "https://github.com/will-wow/natural-language-commander",
  "bugs": "https://github.com/will-wow/natural-language-commander/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/will-wow/natural-language-commander.git"
  },
  "main": "dist/nlc.js",
  "scripts": {
    "test": "mocha --opts ./mocha.opts",
    "build": "tsc && NODE_ENV=production browserify ./dist/NaturalLanguageCommander.js -t [ babelify --presets [ es2015 ] --plugins [ transform-runtime ] ] --standalone NLC | derequire > dist/nlc.js && uglifyjs dist/nlc.js -o dist/nlc.min.js",
    "prebuild": "rimraf dist && mkdirp dist",
    "postbuild": "rimraf dist/test",
    "prepublish": "npm run build",
    "format": "yarn format-tslint && yarn format-prettier",
    "format-tslint": "tslint -p . --config tslint.json",
    "format-prettier": "prettier --write \"**/*.{ts,json}\""
  },
  "keywords": [
    "natural language processing",
    "nlp",
    "commands",
    "bots",
    "hubot"
  ],
  "author": "Will Ockelmann-Wagner <will.ockelmann.wagner@gmail.com> (https://will-wow.surge.sh)",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/chai-spies": "^0.0.0",
    "@types/mocha": "^5.0.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "chai": "^4.1.2",
    "chai-spies": "^1.0.0",
    "derequire": "^2.0.3",
    "mkdirp": "^0.5.1",
    "mocha": "^5.0.5",
    "prettier": "^1.11.1",
    "rimraf": "^2.6.2",
    "ts-node": "^5.0.1",
    "tsify": "^3.0.4",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.8.0",
    "tslint-config-prettier": "^1.10.0",
    "typescript": "^3.8.3",
    "uglify-js": "^2.6.2",
    "uglifyify": "^3.0.1"
  },
  "dependencies": {
    "@types/lodash": "^4.14.106",
    "babel-runtime": "^6.9.2",
    "lodash": "^4.17.5",
    "moment": "^2.22.0",
    "moment-timezone": "^0.5.14"
  },
  "typings": "./dist/NaturalLanguageCommander.d.ts",
  "engines": {
    "node": ">=4.0.0"
  }
}
